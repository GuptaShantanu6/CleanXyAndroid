<?xml version="1.0" encoding="utf-8"?>

<com.sothree.slidinguppanel.SlidingUpPanelLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:sothree="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".bottomNavFragments.HomeFragment"
    android:gravity="bottom"
    android:id="@+id/slidingLayoutHomeFragment"
    sothree:umanoPanelHeight="40dp"
    sothree:umanoShadowHeight="0dp"
    sothree:umanoOverlay="true"
    sothree:umanoParallaxOffset="120dp"
    sothree:umanoDragView="@id/dragView">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.google.android.gms.maps.MapView
            android:id="@+id/map_view_home_fragment"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/placeSearchStart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.03"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/placeSearchEnd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.85"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/placeSearchTop"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.02"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/placeSearchBottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.08"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/searchLocationTextTop"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.033"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/searchLocationTextBottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.067"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/searchLocationTextStart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.1"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/searchLocationTextEnd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.71"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toEndOf="@id/searchLocationTextStart"
            app:layout_constraintEnd_toEndOf="@id/searchLocationTextEnd"
            app:layout_constraintTop_toBottomOf="@id/searchLocationTextTop"
            app:layout_constraintBottom_toTopOf="@id/searchLocationTextBottom"
            android:textColor="@color/black"
            android:autoSizeTextType="uniform"
            android:gravity="center_vertical"
            android:text="Search Location"
            android:elevation="11dp"
            android:fontFamily="@font/robotoregular" />

        <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="@id/searchLocationTextEnd"
            app:layout_constraintEnd_toEndOf="@id/placeSearchEnd"
            app:layout_constraintTop_toBottomOf="@id/searchLocationTextTop"
            app:layout_constraintBottom_toTopOf="@id/searchLocationTextBottom"
            android:src="@drawable/ic_baseline_search_24"
            android:elevation="11dp"/>

        <View
            android:id="@+id/searchLocationBarHomeFragment"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@id/placeSearchTop"
            app:layout_constraintBottom_toTopOf="@id/placeSearchBottom"
            app:layout_constraintStart_toEndOf="@id/placeSearchStart"
            app:layout_constraintEnd_toEndOf="@id/placeSearchEnd"
            android:elevation="10dp"
            android:background="@drawable/rounded_rectangle_home"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/dragView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:elevation="15dp"
        android:background="@drawable/round_background_slidepanel_homefragment">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/barSlidingPanelHomeFragmentStart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.35"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/barSlidingPanelHomeFragmentEnd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.65"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/barSlidingPanelHomeFragmentTop"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.01"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/barSlidingPanelHomeFragmentBottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.025"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/compNameTop"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.95"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/compNameBottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.98"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/servicesTextSlidePanelHomeFragmentTop"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.06"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/servicesTextSlidePanelHomeFragmentBottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.095"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/servicesTextSlidePanelHomeFragmentStart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.08"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/servicesTextSlidePanelHomeFragmentEnd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.33"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/servicesSlidePanelHomeFragmentTop"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.11"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/servicesSlidePanelHomeFragmentBottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.25"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/serviceFirstSlidePanelHomeFragmentEnd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.3"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/serviceSecondSlidePanelHomeFragmentStart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.39"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/serviceSecondSlidePanelHomeFragmentEnd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.61"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/serviceThirdSlidePanelHomeFragmentStart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.7"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/serviceThirdSlidePanelHomeFragmentEnd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.92"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/bookBtnSlidePanelHomeFragmentTop"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.28"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/bookBtnSlidePanelHomeFragmentBottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.35"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/orTextSlidePanelHomeFragmentTop"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.36"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/orTextSlidePanelHomeFragmentBottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.39"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/orTextSlidePanelHomeFragmentStart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.47"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/orTextSlidePanelHomeFragmentEnd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.53"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/scheduleBtnSlidePanelHomeFragmentTop"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.4"/>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/scheduleBtnSlidePanelHomeFragmentBottom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.47"/>

        <View
            android:id="@+id/firstSelectedSlidePanelHomeFragment"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="@id/serviceFirstSlidePanelHomeFragment"
            app:layout_constraintBottom_toBottomOf="@id/serviceFirstSlidePanelHomeFragment"
            app:layout_constraintStart_toStartOf="@id/serviceFirstSlidePanelHomeFragment"
            app:layout_constraintEnd_toEndOf="@id/serviceFirstSlidePanelHomeFragment"
            android:elevation="25dp"
            android:foreground="@drawable/my_button"/>

        <View
            android:id="@+id/secondSelectedSlidePanelHomeFragment"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="@id/serviceSecondSlidePanelHomeFragment"
            app:layout_constraintBottom_toBottomOf="@id/serviceSecondSlidePanelHomeFragment"
            app:layout_constraintStart_toStartOf="@id/serviceSecondSlidePanelHomeFragment"
            app:layout_constraintEnd_toEndOf="@id/serviceSecondSlidePanelHomeFragment"
            android:elevation="25dp"
            android:foreground="@drawable/my_button"/>

        <View
            android:id="@+id/thirdSelectedSlidePanelHomeFragment"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="@id/serviceThirdSlidePanelHomeFragment"
            app:layout_constraintBottom_toBottomOf="@id/serviceThirdSlidePanelHomeFragment"
            app:layout_constraintStart_toStartOf="@id/serviceThirdSlidePanelHomeFragment"
            app:layout_constraintEnd_toEndOf="@id/serviceThirdSlidePanelHomeFragment"
            android:elevation="25dp"
            android:foreground="@drawable/my_button" />

        <Button
            android:id="@+id/scheduleBtnSlidePanelHomeFragment"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="@id/servicesTextSlidePanelHomeFragmentStart"
            app:layout_constraintEnd_toEndOf="@id/serviceThirdSlidePanelHomeFragmentEnd"
            app:layout_constraintTop_toBottomOf="@id/scheduleBtnSlidePanelHomeFragmentTop"
            app:layout_constraintBottom_toTopOf="@id/scheduleBtnSlidePanelHomeFragmentBottom"
            style="@style/Widget.MaterialComponents.Button.TextButton"
            android:foreground="@drawable/my_button"
            android:fontFamily="@font/ptsans"
            android:textColor="@color/appBlue"
            android:text="Schedule Slot"
            android:textAllCaps="false"
            android:textSize="20sp"
            android:textAppearance="?attr/textAppearanceBody1" />

        <TextView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@id/orTextSlidePanelHomeFragmentTop"
            app:layout_constraintBottom_toTopOf="@id/orTextSlidePanelHomeFragmentBottom"
            app:layout_constraintStart_toStartOf="@id/orTextSlidePanelHomeFragmentStart"
            app:layout_constraintEnd_toEndOf="@id/orTextSlidePanelHomeFragmentEnd"
            android:fontFamily="@font/ptsans"
            android:textColor="@color/appBlue"
            android:gravity="center"
            android:autoSizeTextType="uniform"
            android:text="Or"
            android:textAllCaps="false"/>

        <Button
            android:id="@+id/bookBtnSlidePanelHomeFragment"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="@id/bookBtnSlidePanelHomeFragmentTop"
            app:layout_constraintBottom_toTopOf="@id/bookBtnSlidePanelHomeFragmentBottom"
            app:layout_constraintStart_toStartOf="@id/servicesTextSlidePanelHomeFragmentStart"
            app:layout_constraintEnd_toEndOf="@id/serviceThirdSlidePanelHomeFragmentEnd"
            style="@style/Widget.MaterialComponents.Button.TextButton"
            android:foreground="@drawable/my_button"
            android:fontFamily="@font/ptsans"
            android:textColor="@color/appBlue"
            android:textSize="20sp"
            android:text="Book Now"
            android:textAllCaps="false"
            android:textAppearance="?attr/textAppearanceBody1"/>

        <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="@id/serviceFirstSlidePanelHomeFragment"
            app:layout_constraintBottom_toBottomOf="@id/serviceFirstSlidePanelHomeFragment"
            app:layout_constraintStart_toStartOf="@id/serviceFirstSlidePanelHomeFragment"
            app:layout_constraintEnd_toEndOf="@id/serviceFirstSlidePanelHomeFragmentEnd"
            android:src="@drawable/ic_vaccum_cleaner"
            android:elevation="25dp"
            android:padding="10dp" />

        <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="@id/serviceSecondSlidePanelHomeFragment"
            app:layout_constraintBottom_toBottomOf="@id/serviceSecondSlidePanelHomeFragment"
            app:layout_constraintStart_toStartOf="@id/serviceSecondSlidePanelHomeFragment"
            app:layout_constraintEnd_toEndOf="@id/serviceSecondSlidePanelHomeFragmentEnd"
            android:src="@drawable/ic_washing_machine"
            android:elevation="25dp"
            android:padding="10dp"/>

        <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="@id/serviceThirdSlidePanelHomeFragment"
            app:layout_constraintBottom_toBottomOf="@id/serviceThirdSlidePanelHomeFragment"
            app:layout_constraintStart_toStartOf="@id/serviceThirdSlidePanelHomeFragment"
            app:layout_constraintEnd_toEndOf="@id/serviceThirdSlidePanelHomeFragmentEnd"
            android:elevation="25dp"
            android:padding="10dp"
            android:src="@drawable/ic_cleaning_spray_bottle"/>

        <View
            android:id="@+id/serviceFirstSlidePanelHomeFragment"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:elevation="20dp"
            app:layout_constraintTop_toBottomOf="@id/servicesSlidePanelHomeFragmentTop"
            app:layout_constraintBottom_toTopOf="@id/servicesSlidePanelHomeFragmentBottom"
            app:layout_constraintStart_toStartOf="@id/servicesTextSlidePanelHomeFragmentStart"
            app:layout_constraintEnd_toEndOf="@id/serviceFirstSlidePanelHomeFragmentEnd"
            android:background="@drawable/rounded_square_slidepanel_homefragment"/>

        <View
            android:id="@+id/serviceSecondSlidePanelHomeFragment"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:elevation="20dp"
            app:layout_constraintTop_toBottomOf="@id/servicesSlidePanelHomeFragmentTop"
            app:layout_constraintBottom_toTopOf="@id/servicesSlidePanelHomeFragmentBottom"
            app:layout_constraintStart_toStartOf="@id/serviceSecondSlidePanelHomeFragmentStart"
            app:layout_constraintEnd_toEndOf="@id/serviceSecondSlidePanelHomeFragmentEnd"
            android:background="@drawable/rounded_square_slidepanel_homefragment"/>



        <View
            android:id="@+id/serviceThirdSlidePanelHomeFragment"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:elevation="20dp"
            app:layout_constraintTop_toBottomOf="@id/servicesSlidePanelHomeFragmentTop"
            app:layout_constraintBottom_toTopOf="@id/servicesSlidePanelHomeFragmentBottom"
            app:layout_constraintStart_toStartOf="@id/serviceThirdSlidePanelHomeFragmentStart"
            app:layout_constraintEnd_toEndOf="@id/serviceThirdSlidePanelHomeFragmentEnd"
            android:background="@drawable/rounded_square_slidepanel_homefragment"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@id/servicesTextSlidePanelHomeFragmentTop"
            app:layout_constraintBottom_toTopOf="@id/servicesTextSlidePanelHomeFragmentBottom"
            app:layout_constraintStart_toStartOf="@id/servicesTextSlidePanelHomeFragmentStart"
            app:layout_constraintEnd_toEndOf="@id/servicesTextSlidePanelHomeFragmentEnd"
            android:gravity="center_vertical"
            android:fontFamily="@font/ptsans"
            android:textColor="@color/appBlue"
            android:autoSizeTextType="uniform"
            android:text="Services :"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@id/compNameTop"
            app:layout_constraintBottom_toTopOf="@id/compNameBottom"
            app:layout_constraintStart_toStartOf="@id/barSlidingPanelHomeFragmentStart"
            app:layout_constraintEnd_toEndOf="@id/barSlidingPanelHomeFragmentEnd"
            android:fontFamily="@font/ptsans"
            android:textColor="@color/quantum_grey"
            android:gravity="center"
            android:autoSizeTextType="uniform"
            android:text="CleanXy Ⓒ "/>

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@id/barSlidingPanelHomeFragmentTop"
            app:layout_constraintBottom_toTopOf="@id/barSlidingPanelHomeFragmentBottom"
            app:layout_constraintStart_toStartOf="@id/barSlidingPanelHomeFragmentStart"
            app:layout_constraintEnd_toEndOf="@id/barSlidingPanelHomeFragmentEnd"
            android:background="@drawable/bar_slidingpanel_homefragment"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

</com.sothree.slidinguppanel.SlidingUpPanelLayout>