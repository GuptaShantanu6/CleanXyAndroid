    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:autoSizeTextType="uniform"
        android:fontFamily="@font/ptsans"
        android:gravity="center_vertical"
        android:text="back"
        android:textColor="@color/appBlue"
        app:layout_constraintBottom_toTopOf="@+id/guideline125"
        app:layout_constraintEnd_toStartOf="@+id/guideline128"
        app:layout_constraintStart_toStartOf="@+id/guideline129"
        app:layout_constraintTop_toTopOf="@+id/guideline124" />

    <ImageButton
        android:id="@+id/imageButton_timer"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/rounded_circle"
        android:src="@drawable/ic_baseline_timer_24"
        app:layout_constraintBottom_toTopOf="@+id/guideline131"
        app:layout_constraintEnd_toStartOf="@+id/guideline132"
        app:layout_constraintStart_toStartOf="@+id/guideline133"
