        android:id="@+id/doneBtnStart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.02"/>

    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/doneBtnTop"
        app:layout_constraintBottom_toTopOf="@id/doneBtnBottom"
        app:layout_constraintStart_toEndOf="@id/doneBtnStart"
        app:layout_constraintEnd_toStartOf="@id/doneBtnEnd"
